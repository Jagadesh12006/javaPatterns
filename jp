        Scanner scan = new Scanner(System.in);
        System.out.print("Enter the number : ");
        int n = scan.nextByte();

        for(int i = 1; i <= n; i++)
        {
            for(int j = 1; j <= i; j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

